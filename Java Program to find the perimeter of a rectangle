import java.util.Scanner;
public class Main
{
    public static void main(String[] args)
    {
        int l,w,p;
       Scanner sc=new Scanner(System.in);
       System.out.println("Enter Length and Breadth of Rectangle");
       l=sc.nextInt();
       w=sc.nextInt();
       p=2*(l+w);
       System.out.println("Perimeter of Rectangle= "+p);

    }
}
